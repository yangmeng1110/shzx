<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shzx.role.entity.RoleInfo">
   
   <select id="selectRoleInfoById" resultType="roleInfo" parameterType="java.lang.String">
   		SELECT 
  			ID,ROLENAME
  			FROM ROLE_INFO
  		WHERE ID = #{id,jdbcType=VARCHAR}	
   </select>
    
</mapper>