<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shzx.role.entity.RoleFunc">
   
   <select id="selectRoleFuncById" resultType="roleFunc" parameterType="java.lang.String">
   		SELECT 
  			ID,ROLEID,GROUPID
  			FROM ROLE_FUNC
  		WHERE ROLEID = #{roleId,jdbcType=VARCHAR}	
   </select>
    
</mapper>